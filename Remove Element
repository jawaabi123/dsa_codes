https://leetcode.com/problems/remove-element/description/?envType=study-plan-v2&envId=top-interview-150
code:
class Solution {
public:
    int removeElement(vector<int>& nums, int val) 
    {
        int k=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]!=val) 
            {
                nums[k] = nums[i];
                k++;
            }
            
        }
        return k;
    }
};
